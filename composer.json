{
  "name": "uafrica/delayed-jobs",
  "description": "A CakePHP Plugin to manage and process background jobs with priority and retries",
  "type": "cakephp-plugin",
  "require": {
    "cakephp/cakephp": "~3.0.0",
    "friendsofcake/crud": "~4.0",
    "cakephp/migrations": "~1.0",
    "friendsofcake/process-mq": "dev-master",
    "friendsofcake/process-manager": "dev-master"
  },
  "require-dev": {
    "cakephp/app": "~3.0",
    "phpunit/phpunit": "~4.5",
    "sizuhiko/fabricate": "v2.x-dev",
    "sizuhiko/cake_fabricate": "dev-develop"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jaco Roux",
      "email": "jaco@uafrica.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DelayedJobs\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DelayedJobs\\Test\\": "tests",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "friendsofcake/process-mq",
        "version": "dev-master",
        "source": {
          "url": "git@github.com:FriendsOfCake/process-mq.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "psr-4": {
            "ProcessMQ\\": "src"
          }
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "friendsofcake/process-manager",
        "version": "dev-master",
        "source": {
          "url": "git@github.com:FriendsOfCake/process-manager.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "psr-4": {
            "ProcessManager\\": "src"
          }
        }
      }
    }
  ]
}
